{
	"name" : "Songza2Spotify",
	"version" : "0.1",
	"description" : "Make Spotify playlists from Songza playlists",
	"content_scripts" : [
		{
			"matches" : ["http://songza.com/*"],
			"js" : ["jquery-1.11.1.min.js", "main.js"]
		}
	],
	"icons": { 
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},
	"manifest_version" : 2
}